{
 "manifest_version": 2,
 "name": "Aston Wallpapers",
 "description": "Replaces your new tab windows with beautiful landscape photos.",
 "version": "1",
 "author": "Agoston Fung",
"browser_action": {
   "default_icon": "logo.png",
   "default_title": "Have a good day"
 },
"chrome_url_overrides" : {
  "newtab": "newtab.html"
},
 "permissions": ["activeTab"],
 "content_security_policy": "script-src 'self' 'unsafe-inline' https://*.googleapis.com;"
}